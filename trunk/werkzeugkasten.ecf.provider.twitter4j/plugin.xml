<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ecf.containerFactory">
      <containerFactory
            class="werkzeugkasten.ecf.provider.twitter4j.container.TwitterContainerInstantiator"
            description="Twitter"
            name="werkzeugkasten.ecf.provider.twitter4j">
      </containerFactory>
   </extension>
   <extension
         point="org.eclipse.ecf.identity.namespace">
      <namespace
            class="werkzeugkasten.ecf.provider.twitter4j.identity.TwitterNamespace"
            name="werkzeugkasten.ecf.provider.twitter4j">
      </namespace>
   </extension>

</plugin>
