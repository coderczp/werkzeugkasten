<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="werkzeugkasten.twowaysql.editor.TwoWaySqlEditorContainer"
            extensions="sql"
            icon="icons/sql.gif"
            id="werkzeugkasten.twowaysql.editor"
            name="TwoWaySqlEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="werkzeugkasten.twowaysql.editor.conf.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            class="werkzeugkasten.twowaysql.editor.TwoWaySqlDocumentSetupParticipant"
            extensions="sql">
      </participant>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerSeverity="2"
            markerType="org.eclipse.core.resources.problemmarker"
            name="werkzeugkasten.twowaysql.plugin.annotation.error"
            super="org.eclipse.ui.workbench.texteditor.error">
      </type>
   </extension>

</plugin>
